   "source": [
    "#derive column discount , sale price and profit\n",
    "df['discount'] = df['list_price']*df['discount_percent']/100\n",
    "df['sale_price']= df['list_price']-df['discount']\n",
    "df['profit'] = df['sale_price']-df['cost_price']\n",
    "df.head(10)"
   ]
